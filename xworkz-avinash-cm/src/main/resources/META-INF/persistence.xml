            <property name="javax.persistence.jdbc.url"    value="jdbc:mysql://Localhost:3306/ai_db" />
            <property name="javax.persistence.jdbc.user" value="root" />
            <property name="javax.persistence.jdbc.password" value="Avinash42464225" />
            <property name="hibernate.dialect" value="org.hibernate.dialect.MySQL8Dialect"/>
            
            <property name="show_sql" value="true"/>
        </properties>
    </persistence-unit>
</persistence> 